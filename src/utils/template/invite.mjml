<mjml>
  <mj-head>
    <mj-style>
      .link-center {
        color: #06C953;
        text-decoration: none;
      }

      .star {
        height: 15px;
        width: 15px;
      }

      .btn-href {
        border-radius: 4px;
        background-color: #297bf6;
      }
    </mj-style>
  </mj-head>
  <mj-body background-color="#f2f2f2">
    <mj-section padding="40px 42px">
      <mj-column>
        <mj-image padding="0" src="data:image/svg+xml;base64," alt="" width="176.1px" align="center" />
      </mj-column>
    </mj-section>
    <mj-section padding="0 40px 33px 40px">
      <mj-column background-color="#fff" padding-bottom="60px" border-radius="6px">
        <mj-text padding="55px 10px 0 10px" align="center" font-size="24px" font-weight="bold" color="#37373a" font-family="helvetica">მოწვევა</mj-text>
        <mj-text font-size="16px" color="#37373a" line-height="30px" font-family="helvetica" padding="30px 22px 50px 22px" align="center">
          ცნობილი ფაქტია, რომ გვერდის წაკითხვად შიგთავსს შეუძლია მკითხველისცნობილი ფაქტია, რომ გვერდის წაკითხვად შიგთავსს შეუძლია
          {{text}}
        </mj-text>
      </mj-column>
    </mj-section>
    <mj-section padding="0">
      <mj-column width="35px">
        <mj-image href="https://facebook.com" padding="2px 0 0 0" src="http://www.picz.ge/img/s4/1904/16/7/7b19eb404688.png" alt="" width="13px" />
      </mj-column>
      <mj-column width="35px">
        <mj-image href="https://instagram.com" padding="0" src="http://www.picz.ge/img/s2/1904/16/f/ff24cfe1525f.png" alt="" width="24px" />
      </mj-column>
    </mj-section>
    <mj-section padding="0">
      <mj-column>
        <mj-navbar base-url="/">
          <mj-navbar-link href="/" font-size="14px" font-weight="bold" font-family="helvetica" color="#37373a" text-transform="lowercase">www.localhost:8000</mj-navbar-link>
        </mj-navbar>
      </mj-column>
    </mj-section>
  </mj-body>
</mjml>
